{
  "parser": "@typescript-eslint/parser",
  "extends": ["airbnb", "next/core-web-vitals", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "parserOptions": {
        "project": ["./tsconfig.json"]
      }
    }
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".tsx"] }]
  }
}
